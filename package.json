{
  "name": "vue-vite-template",
  "version": "1.0.0-rc.1",
  "gitHooks": {
    "pre-commit": "yarn lint",
    "pre-push": "yarn rsc"
  },
  "scripts": {
    "vite-dev-server": "vite",
    "vite-build": "vite build",
    "start": "dotenv -e .env.local yarn vite-dev-server",
    "build": "dotenv -e .env.production yarn vite-build",
    "analyze": "dotenv -e .env.analyze yarn vite-build",
    "lint": "yarn lint:styles && yarn lint:scripts",
    "lint:styles": "stylelint \"src/**/*.{vue,scss,css}\" --max-warnings=0",
    "lint:scripts": "eslint \"{src,config}/**/*.{vue,js,ts}\" --cache --fix --max-warnings=0 && tsc --noEmit",
    "preview": "vite preview",
    "rsc": "node scripts/release-sanity-check.mjs"
  },
  "dependencies": {
    "@distributedlab/tools": "^1.0.0-rc.8",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.6.1",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "pinia": "^2.1.7",
    "postcss": "^8.4.31",
    "uuid": "^9.0.1",
    "vue": "^3.3.8",
    "vue-i18n": "^9.6.5",
    "vue-router": "^4.2.5",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@types/vuelidate": "^0.7.21",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.53.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-declaration-strict-value": "^1.9.2",
    "stylelint-scss": "^5.3.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-node-polyfills": "^0.16.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^9.3.2",
    "yorkie": "^2.0.0"
  }
}
